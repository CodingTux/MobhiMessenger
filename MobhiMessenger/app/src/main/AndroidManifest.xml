<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mobhimessenger">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/iconnew"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/iconnew_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ChatLogActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".LatestMessagesActivity"/>
        </activity>
        <activity android:name=".NewMessageActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".LatestMessagesActivity"/>
        </activity>
        <activity android:name=".LatestMessagesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".LoginActivity" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
    </application>

</manifest>